<?xml version="1.0" encoding="utf-8"?>
<resources>
	
    <!-- You can add multiple backs and masks but keep in mind
    	not all launchers are compatible with adding all of these features -->
    
    <iconback img1="iconback"/>
    <iconupon img1="iconupon"/>
    
	
    <!-- Remove "iconupon" and "iconmask" for better Go Launcher compatibility 
    	failure to remove these will cause Go launcher to use them incorrectly 
    	and they will be reversed from every other launcher -->
    <!--
    <iconupon 
    img1="iconupon"
    img2="iconupon1"
    /> -->
    
    
	<iconmask img1="iconmask" />
 	
    <!-- This scales the images to fit into your iconback -->
    <scale factor="0.8" />
	
    <!-- NOVA LAUNCHER STUFF -->
    <item component=":LAUNCHER_ACTION_APP_DRAWER" drawable="app_drawer" />
    <item component=":BROWSER" drawable="browser" />
    <item component=":SMS" drawable="sms" />
    
    
</resources>